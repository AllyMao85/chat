#main-header
	height 85px
	background #333
	background -moz-linear-gradient(top, #333 0%, #282828 40%, #1e1e1e 100%)
	background -webkit-gradient(linear, left top, left bottom, color-stop(0%, #333), color-stop(40%, #282828), color-stop(100%, #1e1e1e))
	background -webkit-linear-gradient(top, #333 0%, #282828 40%, #1e1e1e 100%)
	background -o-linear-gradient(top, #333 0%, #282828 40%, #1e1e1e 100%)
	background -ms-linear-gradient(top, #333 0%, #282828 40%, #1e1e1e 100%)
	background linear-gradient(top, #333 0%, #282828 40%, #1e1e1e 100%)
	nav
		color #fff
		ul
			list-style-type none
			display inline-block
			margin 0px 0px 0px 90px
			li
				display inline-block
				line-height 85px
				height 85px
				margin-right 30px
				margin-bottom 0px
				color #ddd
				font-size 18px
				&:hover
					color: #FFF

	#header-content
		position relative
		width 70%
		margin 0px auto
		.logoArea
			float left
			img
				width 100px
				height 100px
	    .high
			float right

#sub-header
	height 40px
	background-color #f5f5f5
	border-bottom 1px solid #e2e2e2
